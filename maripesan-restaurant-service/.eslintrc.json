{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "import/extensions": "off",
    "radix": "off",
    "no-restricted-globals": "off",
    "consistent-return": "off",
  }
}
